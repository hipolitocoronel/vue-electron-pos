{
  "name": "vue-electron-taller",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm run init-server\" \"vue-cli-service serve\"",
    "serve-mac": "concurrently \"npm run init-server-mac\" \"vue-cli-service serve\"",
    "build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "init-server": "pocketbase.exe serve",
    "init-server-mac": "./pocketbase serve",
    "start": "concurrently \"npm run init-server\" \"npm run electron:serve\"",
    "start-mac": "concurrently \"npm run init-server-mac\" \"npm run electron:serve\""
  },
  "main": "background.js",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "axios": "^1.2.1",
    "concurrently": "^7.6.0",
    "core-js": "^3.8.3",
    "moment": "^2.29.4",
    "pocketbase": "^0.9.0",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vue-toastification": "^1.7.14",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^12.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint-config-prettier": "^8.6.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
